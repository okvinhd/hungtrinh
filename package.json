    "cookie-universal-nuxt": "^2.2.2",
    "core-js": "^3.19.3",
    "element-ui": "^2.15.6",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "qrcode.vue": "^1.7.0",
    "typescript": "^4.8.3",
    "vue": "^2.6.14",
